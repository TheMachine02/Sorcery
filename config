define CONFIG_KERNEL_NAME		"Sorcery.0.0.1"

; special code modifier ;
define	CONFIG_USE_BOOT_PATCH		0
define	CONFIG_USE_DOWNCLOCKING		0
define	CONFIG_USE_RAM_CACHE		1
define	CONFIG_FPU_FAULT		1
define	CONFIG_USE_LZ4_STRICT		0

; clock settings ;
define	CONFIG_CRYSTAL_DIVISOR		3
; 0 = div 74
; 1 = div 154
; 2 = div 218 (or about 150 Hz)
; 3 = div 314 (or about 105 Hz)

; syscall ;
define	CONFIG_STATIC_SYSCALL		1
define	CONFIG_STATIC_LIBRARY		0
